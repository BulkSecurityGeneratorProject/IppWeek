entity Ville {
    image Blob
	nom String
    pays String
    description String
}

entity Site {
	nom String
    lienUrl String
    prix Long
    description String
    adresse String
}

entity Parcours {
   titre String
}

relationship OneToMany {
   Site{ville(nom)} to Ville
}

relationship ManyToMany {
  Parcours{site(nom)} to Site{parcours(titre)}
}

relationship OneToMany {
  Ville{parcours(titre)} to Parcours
}



// Set service options to all except few
service all with serviceImpl

